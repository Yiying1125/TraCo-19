    <style name="Theme.TraCo" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/mmuColourSecondary</item>

    </style>


    <style name="Theme.TraCo19" parent="Theme.AppCompat.Light.DarkActionBar">
